<%-include('./partials/header')%>
<link rel="stylesheet" href="/css/login.css">
<%-include('./partials/header2')%>

<div class="login-container"> 
        <h1>Login</h1>
        <p class="error"></p>
        <form id="login-form">

            <label for="userName">User Name: </label> <input type="text" name="userName" autocomplete="off" required placeholder="User Name"> 
            <label for="password">Password </label> <input type="password" name="password" autocomplete="off" required placeholder="Password"> 
            <button class="btn btn-orange btn-large btn-center">Login</button>
        
        </form>
        
        <a href="/register">Register</a>
</div>

<script src="/js/login.js"></script>

<%-include('./partials/footer')%>